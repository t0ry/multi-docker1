{
 "AWSEBDuckerrunVersion": 2,
 "containerDefinitions": [
   {
     "name": "client",
     "image": "tory00b/multu-client",
     "hostname": "client",
     "essential" : false
   },
   {
     "name": "server",
     "image": "tory00b/multu-server",
     "hostname": "api",
     "essential": false
  },
  {
     "name": "worker",
     "image": "tory00b/multu-worker",
     "hostname": "worker",
     "essential": false
  
  },
  {
     "name": "nginx",
     "image": "tory00b/multu-nginx",
     "essential": true,
     "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client", "server"]

   }
 ]
}
