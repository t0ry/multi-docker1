{
 "AWSEBDockerrunVersion": 2,
 "containerDefinitions": [
   {
     "name": "client",
     "image": "tory00b/multu-client",
     "hostname": "client",
     "essential" : false,
     "memory": 128
   },
   {
     "name": "server",
     "image": "tory00b/multu-server",
     "hostname": "api",
     "essential": false,
     "memory": 128
  },
  {
     "name": "worker",
     "image": "tory00b/multu-worker",
     "hostname": "worker",
     "essential": false,
     "memory": 128

  },
  {
     "name": "nginx",
     "image": "tory00b/multu-nginx",
     "essential": true,
     "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "links": ["client", "server"],
      "memory": 128
   }
 ]
}
